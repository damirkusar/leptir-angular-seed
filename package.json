    "angular-resource": "./public/bower_components/angular-resource/angular-resource.min.js",
    "angular-ui-router": "./public/bower_components/angular-ui-router/release/angular-ui-router.min.js",
    "angular-ui-utils": "./public/bower_components/angular-ui-utils/ui-utils.min.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "bootstrap": {
      "exports": "bootstrap"
    },
    "moment": {
      "exports": "moment"
    },
    "underscore": {
      "exports": "underscore"
