    },
    "angular-bootstrap": {
      "depends": ["angular"],
      "exports": "null"
    },
    "angular-bootstrap-tpls": {
      "depends": ["angular"],
      "exports": "null"
    },
    "angular-cookies": {
      "depends": ["angular"],
      "exports": "null"
    },
    "angular-mocks": {
      "depends": ["angular"],
      "exports": "null"
    },
    "angular-resource": {
      "depends": ["angular"],
      "exports": "null"
    },
    "angular-ui-router": {
      "depends": ["angular"],
      "exports": "null"
    },
    "angular-ui-utils": {
      "depends": ["angular"],
      "exports": "null"
